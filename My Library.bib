@article{Li2014Miniature,
  title = {Miniature Forward-Imaging {{B-scan}} Optical Coherence Tomography Probe to Guide Real-Time Laser Ablation: {{MINIATURE B-SCAN FORWARD-IMAGING OCT PROBE}}},
  shorttitle = {Miniature Forward-Imaging {{B-scan}} Optical Coherence Tomography Probe to Guide Real-Time Laser Ablation},
  author = {Li, Zhuoyan and Shen, Jin H. and Kozub, John A. and Prasad, Ratna and Lu, Pengcheng and Joos, Karen M.},
  year = {2014},
