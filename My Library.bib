  title = {Optical Coherence Tomography-Guided Laser Marking with Tethered Capsule Endomicroscopy in Unsedated Patients},
  author = {Liang, Chia-Pin and Dong, Jing and Ford, Tim and Reddy, Rohith and Hosseiny, Hamid and Farrokhi, Hamid and Beatty, Matthew and Singh, Kanwarpal and Osman, Hany and Vuong, Barry and Baldwin, Grace and Grant, Catriona and Giddings, Sarah and Gora, Michalina J. and Rosenberg, Mireille and Nishioka, Norman and Tearney, Guillermo},
