@article{Kohli2017Performance,
  title = {Performance Characteristics of Optical Coherence Tomography in Assessment of {{Barrett}}'s Esophagus and Esophageal Cancer: Systematic Review},
  shorttitle = {Performance Characteristics of Optical Coherence Tomography in Assessment of {{Barrett}}'s Esophagus and Esophageal Cancer},
  author = {Kohli, D. R. and Schubert, M. L. and Zfass, A. M. and Shah, T. U.},
