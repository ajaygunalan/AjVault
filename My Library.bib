  shorttitle = {Smart Laser Osteotomy},
  author = {Jivraj, Jamil and Chen, Chaoliang and Huang, Yize and Ramjist, Joel and Lu, Yi and Vuong, Barry and Gu, Xijia and Yang, Victor X. D.},
  year = {2018},
