@article{Ughi2016Automated,
  title = {Automated Segmentation and Characterization of Esophageal Wall in Vivo by Tethered Capsule Optical Coherence Tomography Endomicroscopy},
  author = {Ughi, Giovanni J. and Gora, Michalina J. and Swager, Anne-Fr{\'e} and Soomro, Amna and Grant, Catriona and Tiernan, Aubrey and Rosenberg, Mireille and Sauk, Jenny S. and Nishioka, Norman S. and Tearney, Guillermo J.},
